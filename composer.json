{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "advoor/nova-resource-field": "^0.3.0",
        "benjaminhirsch/nova-slug-field": "^1.2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2",
        "laravel/nova": "~2.0",
        "laravel/passport": "^8.0",
        "laravel/tinker": "^1.0",
        "neewd/newsletter": "*",
        "neewd/templates": "*",
        "nesbot/carbon": "^2.26",
        "sbine/route-viewer": "^0.0.6",
        "sendgrid/sendgrid": "~7",
        "vansteen/laravel-sendinblue": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "sven/artisan-view": "^3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/Newsletter"
        },
        {
            "type": "path",
            "url": "./nova-components/Newsletter"
        },
        {
            "type": "path",
            "url": "./nova-components/Newsletter"
        },
        {
            "type": "path",
            "url": "./nova-components/Templates"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
